Import('acr')

env = acr.MakeEnv()
env.AppendUnique( LIBS=['bbtrading','bbclientcore', 'bbsimulator', 'bbdb', 'bbcore_msgluabind','bbthreading','tdcore','bbio_mq','bbcore_web' ] + acr.bbio_libs + acr.bbcore_libs )

prog = env.Program(
     target = 'alphaless',
     source = Glob("*.cc")
)

acr.AddBin( prog )
